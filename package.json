{
  "name": "pean-starter",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Adam Podrou≈æek",
  "description": "Full-stack project starter built using Angular 4+, Express, Postgres and Node",
  "scripts": {
    "build:all:dev": "concurrently \"ng build\" \"tsc -p server\"",
    "build:all:prod": "concurrently \"ng build -prod -aot -oh none\" \"tsc -p server\"",
    "watch:all": "concurrently \"ng serve -pc proxy.config.json -dop false -o\" \"tsc -w -p server\" \"nodemon dist/server/server.js\"",
    "run:prod": "npm run build:all:prod && node dist/server/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "body-parser": "^1.18.0",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "csurf": "^1.9.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "font-awesome": "^4.7.0",
    "helmet": "^3.8.1",
    "jquery": "^3.2.1",
    "pg": "^7.3.0",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.2",
    "@angular/compiler-cli": "^4.4.2",
    "@types/body-parser": "^1.16.5",
    "@types/csurf": "^1.9.34",
    "@types/express": "^4.0.37",
    "@types/node": "~6.0.60",
    "@types/pg": "^7.1.4",
    "codelyzer": "^3.2.0",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
